   "source": [
    "# Setting thresholds\n",
    "def income_level(row):\n",
    "    if row['income'] <= 50000:\n",
    "        return 'Low income'\n",
    "    elif (row['income'] > 50000) and (row['income'] <= 100000):\n",
    "        return 'Medium income'\n",
    "    elif row['income'] > 100000:\n",
    "        return 'High income'"
   ]
  },
  {
   "cell_type": "code",
